extends layout

block content
  title = "A Random Item"
  - var image_names = ["camera1.jpg", "camera2.jpg", "drone1.png", "drone2.jpg", "skateboard.jpg"]

  - // Loop three times
  - for (let i = 0; i < 3; i++)
    - let randomIndex = Math.floor(Math.random() * 5)
    - let imageName = image_names[randomIndex]
    img(src=`/images/${imageName}`, alt=imageName, height="200", width="200")
